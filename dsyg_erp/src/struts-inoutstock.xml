<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="inoutstock" extends="struts-erp" namespace="/inoutstock">
	
		<action name="showInOutStockAction" class="inOutStockAction" method="showInOutStockAction">
			<result name="*">/page/inoutstock/inoutstocklist.jsp</result>
		</action>
		
		<action name="queryInOutStockAction" class="inOutStockAction" method="queryInOutStockAction">
			<result name="*">/page/inoutstock/inoutstocklist.jsp</result>
		</action>
		
		<action name="turnInOutStockAction" class="inOutStockAction" method="turnInOutStockAction">
			<result name="*">/page/inoutstock/inoutstocklist.jsp</result>
		</action>
		
		<action name="queryInOutStockByDateAction" class="inOutStockAction" method="queryInOutStockByDateAction">
			<result name="*">/page/inoutstock/inoutstocklist.jsp</result>
		</action>
		
		<action name="showInOutStockDetailAction" class="inOutStockAction" method="showInOutStockDetailAction">
			<result name="*">/page/inoutstock/inoutstockdetail.jsp</result>
		</action>
		
	</package>

</struts>