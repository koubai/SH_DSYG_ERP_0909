	<insert id="insertWarehouse" parameterClass="com.cn.dsyg.dto.WarehouseDto">
		insert into etb_warehouse(parentid,warehousetype,warehousename,warehouseno,belongto,theme1,theme2,productid,quantity,
			actualquantity,unitprice,amount,taxamount,warehousedate,plandate,handler,supplierid,approverid,note,rank,status,
			res01,res02,res03,res04,res05,res06,res07,res08,res09,res10,createuid,createdate,updateuid,updatedate)
		values(#parentid#,#warehousetype#,#warehousename#,#warehouseno#,#belongto#,#theme1#,#theme2#,#productid#,#quantity#,
			#actualquantity#,#unitprice#,#amount#,#taxamount#,#warehousedate#,#plandate#,#handler#,#supplierid#,#approverid#,#note#,#rank#,#status#,
			#res01#,#res02#,#res03#,#res04#,#res05#,#res06#,#res07#,#res08#,#res09#,#res10#,#createuid#,NOW(),#updateuid#,NOW())
		<selectKey resultClass="long" keyProperty="id" >
			select LAST_INSERT_ID() as value
		</selectKey>
	</insert>
	
	<delete id="deleteWarehouseByParentid" parameterClass="java.util.Map">
		DELETE FROM etb_warehouse WHERE parentid = #parentid#
		<isNotEmpty prepend="and" property="productid">
			productid = #productid#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="status">
			status = #status#
		</isNotEmpty>
	</delete>
	
