<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="cuprice" extends="struts-erp" namespace="/cuprice">
	
		<action name="showUpdCuPriceAction" class="cuPriceAction" method="showUpdCuPriceAction">
			<result name="*">/page/cuprice/updcuprice.jsp</result>
		</action>
		
		<action name="updCuPriceAction" class="cuPriceAction" method="updCuPriceAction">
			<result name="*">/page/cuprice/updcuprice.jsp</result>
		</action>
		
		<action name="showAddCuPriceAction" class="cuPriceAction" method="showAddCuPriceAction">
			<result name="*">/page/cuprice/addcuprice.jsp</result>
		</action>
		
		<action name="addCuPriceAction" class="cuPriceAction" method="addCuPriceAction">
			<result name="*">/page/cuprice/addcuprice.jsp</result>
		</action>
		
		<action name="delCuPriceAction" class="cuPriceAction" method="delCuPriceAction">
			<result name="*">/page/cuprice/cupricelist.jsp</result>
		</action>
	
		<action name="showCuPriceAction" class="cuPriceAction" method="showCuPriceAction">
			<result name="*">/page/cuprice/cupricelist.jsp</result>
		</action>
		
		<action name="queryCuPriceAction" class="cuPriceAction" method="queryCuPriceAction">
			<result name="*">/page/cuprice/cupricelist.jsp</result>
		</action>
		
		<action name="turnCuPriceAction" class="cuPriceAction" method="turnCuPriceAction">
			<result name="*">/page/cuprice/cupricelist.jsp</result>
		</action>
		
	</package>

</struts>